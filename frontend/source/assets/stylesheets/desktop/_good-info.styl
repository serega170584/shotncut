

$ctx = '.sb-good-feature'

{$ctx}
  margin-top 48px
  font-family 'Fedra Sans', sans-serif

{$ctx}__icon
  width 48px
  height 48px
  background-size 100%

  &_globe
    background-image url('data:image/png;base64,')

  &_plane
    background-image url('data:image/png;base64,')

  &_moon
    background-image url('data:image/png;base64,')

  &_card
    background-image url('data:image/png;base64,')

{$ctx}__name
  margin-left 16px
  color #333
  font-size 18px
  line-height 24px

{$ctx}__text
  font-size 14px
  line-height 24px
  opacity .5
  padding-left 64px
  margin-top 8px

.sb-good-activate
  background #009942
  color #fff

  .sb-section__caption::before
    position absolute
    left -96px
    width 64px
    background-size 100%
    background-image url('data:image/png;base64,')
    height 64px
    margin-left 0
    content ''

  &__text
    margin-bottom 44px
    line-height 28px

.sb-good-get
  font-size .9375rem
  line-height 24px
  &__icon
    margin-bottom 16px
  &__description
    margin-top 12px


$ctx = '.si-good-accordion'
{$ctx}
  box-shadow 0 1px 0 #e6e6e6

{$ctx}__caption
  font-weight 500
  transition all $transition-duration $transition-timing-function

{$ctx}__pane
  box-shadow 0 -1px 0 #e6e6e6

  &:first-child
    box-shadow none

  &.active
    {$ctx}__head::after
      transform rotate(180deg)


{$ctx}__head
  position relative
  padding 24px 64px 24px 0
  line-height 28px
  cursor pointer

  &::after
    position absolute
    top 36px
    right 24px
    width 14px
    height 8px
    background-image url('data:image/png;base64,')
    background-size 100%
    content ''

{$ctx}__body
  display none
  margin-top -.75rem
  padding 0 4rem 1.5rem 0

  .active &
    display block

{$ctx}_theme_1


  {$ctx}__body
    color #8c8c8c

  {$ctx}__caption
    color #009942
    font-size 20px

  {$ctx}__head:hover
  {$ctx}__pane.active
    {$ctx}__caption
      color #202020


{$ctx}_theme_2
  {$ctx}__body
    color #202020

  {$ctx}__caption
    font-weight 600
    color #202020
    font-size 20px

  {$ctx}__head:hover
  {$ctx}__pane.active
    {$ctx}__caption
      color #009942

  {$ctx}__head,
  {$ctx}__body
    padding-left 56px

.si-good-accordion__badge
  position absolute
  left 0
  top 20px
  min-width 36px
  height 36px
  background #009942
  margin-right 24px
  border-radius: 50%
  text-align center
  line-height 36px
  color #fff
  font-weight 700
  font-size 1.0625rem


.si-good-tabs
  &-nav
    padding-top 20px

    &__button
      color #009942
      margin-left -16px
      margin-bottom 16px

      &.active
        color #000

        &::before
          background #d6e03e

.sb-good-table
  table
    width 100%
    border-top 2px solid #e5e5e5

    tr
      td
        font-size 1.3125rem
        padding 24px 0
        border-bottom 1px solid #e5e5e5
        &:first-child
          font-size .875rem
          text-transform uppercase
          opacity (1/3)
